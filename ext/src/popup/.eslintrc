{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "camelcase": 1,
        "complexity": 1,
        "eqeqeq": 1,
        "no-console": 0,
        "no-empty": 0,
        "no-extra-boolean-cast": 1,
        "no-undef": 0,
        "no-undef-init": 1,
        "no-undefined": 1,
        "no-unreachable": 1,
        "no-unused-vars": 0,
        "semi": 1
    }
}